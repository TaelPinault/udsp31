        <style type="text/css">
            /********************************
                         Reset
            ********************************/
            
            * { box-sizing: border-box; }
            a { color: {{ Custom('color1') }}; text-decoration: none; }
            a:hover { color: {{ Custom('color1') }}; text-decoration: underline; }
            h1, h2, h3, h4, h5 {
                font-family: '{{ Custom('titlesFont') }}';

            }
            html { font-family: '{{ Custom('bodyFont') }}'; }
    
            
            /********************************
                         Body
            ********************************/
            
            .Body {
                color: #606569;
                font-size: 16px;
                line-height: 26px;
                {% if Custom('background_body') == 'Black' %}
                    background-color: #000000;
                {% elseif Custom('background_body') == 'Blue' %}
                    background-color: #6d9bba;
                {% elseif Custom('background_body') == 'Dark-grey' %}
                    background-color: #404040;
                {% elseif Custom('background_body') == 'Green' %}
                    background-color: #a0ba6d;
                {% elseif Custom('background_body') == 'Grey' %}
                    background-color: #858585;
                {% elseif Custom('background_body') == 'Light-grey' %}
                    background-color: #d0d0d0;
                {% elseif Custom('background_body') == 'Midnight-blue' %}
                    background-color: #1c1e2a;
                {% elseif Custom('background_body') == 'Orange' %}
                    background-color: #eeab54;
                {% elseif Custom('background_body') == 'Pink' %}
                    background-color: #d7a4b6;
                {% elseif Custom('background_body') == 'Purple' %}
                    background-color: #a58695;
                {% elseif Custom('background_body') == 'Red' %}
                    background-color: #a02128;
                {% elseif Custom('background_body') == 'White' %}
                    background-color: #ffffff;
                {% else %}
                    background-image : url(http://assets.over-blog.com/t/catalog/images/background/body/{{ Custom('background_body') }}.png);
                {% endif %}
            }
            
            
            body.withsharebar {
                margin-top: 0;
            }
            
            /********************************
                         HeaderNav
            ********************************/
                   
            .HeaderNav {
                display: flex;
                flex-direction: column;
            }
            
            /********************************
                         Header
            ********************************/

            .Header {
                position: relative;
                width: 100%;
                {% if Custom('menu_position') == 'BellowTheHeader' %}
                order: 1;
                {% else %}
                order: 2;
                {% endif %}
            }
            
            body.withsharebar .Header {
                padding-top: 40px;
            }
            
                .Header-content {
                    margin: 10px 0;
                    overflow: hidden;
                    padding: 20px;
                    position: absolute;
                    top: 0;
                    right: 0;
                    left: 0;
                    {% if Custom('title_display') == 'OnAWhiteBox' %}
                    background: #fff;
                    {% elseif Custom('title_display') == 'OnABlackBox' %}
                    background: #000;
                    {% elseif Custom('title_display') == 'OnADarkBox' %}
                    background: rgba(0, 0, 0, .8);
                    {% else %}
                    background: rgba(255, 255, 255, .8);
                    {% endif %}
                }
                
                @media only screen and (min-width: 970px) {
                .Header-content {
                    width: 970px;
                }
                }

                    .Header-title {
                        font-family: '{{ Custom('titlesFont') }}';
                        font-size: 30px;
                        font-weight: 400;
                        vertical-align: middle;
                        text-transform: uppercase;
                        letter-spacing: 2px;
                        margin: 0;
                        line-height: 40px;
                        text-align: center;
                        overflow: hidden;
                        text-overflow: ellipsis;
                        white-space: nowrap;
                    }
                    
                        .Header-titleLink {         
                            -o-transition: .3s;
                            -ms-transition: .3s;
                            -moz-transition: .3s;
                            -webkit-transition: .3s;
                            transition: .3s;
                        }
                        
                        .Header-titleLink:hover {  
                            opacity: 0.5;
                            text-decoration: none;
                        }
                        
                    .Header-subtitle {
                        font-size: 18px;
                        font-weight: 400;
                        vertical-align: middle;
                        margin: 0;
                        line-height: 40px;
                        text-align: center;
                        overflow: hidden;
                        text-overflow: ellipsis;
                        white-space: nowrap;
                        {% if Custom('title_display') == 'OnABlackBox'or Custom('title_display') == 'OnADarkBox' %}
                        color: white
                        {% endif %}
                    }
                    
                .Header-cover {
                    display: block;
                    width: 100%;
                    min-height: 120px;
                    object-fit: cover;
                    padding: 0 10px;
                    margin: auto;
                    position: relative;
                }
                
                @media only screen and (min-width: 970px) {
                .Header-cover {
                    width: 970px;
                    padding: 0;
                    background-color: white;
                }
                }
                
                    .Header-coverImage {
                        display: block;
                        width: 100%;
                    }
    

            /********************************
                         Nav
            ********************************/
            
            .Nav {
                {% if Custom('menu_position') == 'OnTopOfThePage' %}
                order: 1;
                {% else %}
                order: 2;
                {% endif %}
            }
            
                .Nav-content {
                    position: relative;
                    background: #f2f2f2;
                    margin: 0 10px;
                }
                
                @media only screen and (man-width: 970px) {
                .Nav-content {
                    text-align: right;
                }
                }
            
                
                @media only screen and (min-width: 970px) {
                .Nav-content {
                    width: 970px;
                    margin: auto;
                }
                }
            
                    .Nav-menuButton {
                        display: inline-block;
                        width: 50px;
                        height: 50px;
                        line-height: 50px;
                        vertical-align: middle;
                        text-align: center;
                        font-size: 20px;
                        color: white;
                        background: {{ Custom('color1') }};
                        cursor: pointer;
                        -webkit-user-select: none;
                        -moz-user-select: none;
                        -ms-user-select: none;
                        -o-user-select: none;
                        user-select: none; 
                    }
                    
                    @media only screen and (min-width: 970px) {
                    .Nav-menuButton {
                        display: none;
                    }
                    }
                
                
                    .Nav-menu {
                        position: absolute;
                        top: 50px;
                        width: 100%;
                        list-style: none;
                        padding: 0;
                        margin: 0;
                        z-index: 1;
                        display: none;
                    }
                    
                    .Nav-menu--open {
                        display: block;
                    }
                    
                    
                    @media only screen and (min-width: 970px) {
                        .Nav-menu {
                            display: inline-block;
                            position: relative;
                            top: 0;
                            width: auto;
                        }
                    }
                    
                        .NavElement {
                            display: block;
                            list-style: none;
                            height: auto;
                            position: relative;
                            -webkit-transition: all 0.2s;
                            -moz-transition:    all 0.2s;
                            -ms-transition:     all 0.2s;
                            -o-transition:      all 0.2s;
                            transition:         all 0.2s;
                        }
                        
                        @media only screen and (min-width: 970px) {
                        .NavElement {
                            display: inline-block;
                        }
                        }

                            .NavElement-link {
                                display: block;
                                text-decoration: none;
                                color: #000000;
                                background: #f2f2f2;
                                font-family: '{{ Custom('titlesFont') }}';
                                font-size: 14px;
                                text-transform: uppercase;
                                padding: 0 15px;
                                line-height: 45px;
                                border-bottom: 2px solid #f2f2f2;
                            }
                            
                            .NavElement-link:hover {
                                background: #fff;
                                border-bottom: 2px solid {{ Custom('color1') }};
                                
                            }
                            
                            @media only screen and (max-width: 970px) {
                            .NavElement-link {
                                display: block;
                            }
                            }
                            
                            .NavElement-link:hover {
                                text-decoration: none;
                            }
                            
                            
                                .NavElementSub,
                                .NavElementSubSub {
                                    padding-left: 20px;
                                    background-color: black;
                                    -webkit-box-shadow: 0px 0px 4px 0 rgba(0, 0, 0, 0.2);
                                    -moz-box-shadow: 0px 0px 4px 0 rgba(0, 0, 0, 0.2);
                                    box-shadow: 0px 0px 4px 0 rgba(0, 0, 0, 0.2);
                                    background: #ffffff;
                                    list-style: none; 
                                }
                                
                                    .NavElementSub-element {
                                        padding: 0;
                                        display: block;
                                        width: 100%;
                                        position: relative;
                                    }
                                        
                                        .NavElementSub-link,
                                        .NavElementSubSub-link {    
                                            background: #ffffff;
                                            color: #000000;
                                            padding: 0 25px;
                                            font-size: 13px;
                                            text-decoration: none;
                                            font-family: '{{ Custom('titlesFont') }}';
                                        }
                                        
                                        @media only screen and (max-width: 970px) {
                                        .NavElementSub .NavElement-link,
                                        .NavElementSubSub .NavElement-link {
                                            padding: 0 30px 0 30px;
                                        }
                                        }
                                
                                @media only screen and (min-width: 970px) {
                                .NavElementSub,
                                .NavElementSubSub {  
                                    left: 0;
                                    position: absolute;
                                    top: 47px;
                                    text-align: center;
                                    width: 300px;
                                    z-index: 1000;
                                    padding-left: 0;
                                    display: none;
                                    -webkit-transition: all 0.2s;
                                    -moz-transition:    all 0.2s;
                                    -ms-transition:     all 0.2s;
                                    -o-transition:      all 0.2s;
                                    transition:         all 0.2s;
                                }
                                
                                .NavElementSubSub {
                                    left: 300px;
                                    position: absolute;
                                    top: 0;
                                }
                        
                                
                                .NavElementSub-link,
                                .NavElementSubSub-link {
